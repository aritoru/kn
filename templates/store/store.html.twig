{% extends('base.html.twig') %}
{% block store_block %} nav-link-active {% endblock %}
{% block body %}


    <div class="container content">

        <div class="row jumbotron transparent">
            <div class="col">

                <div class="row">
                    {% for product in products %}
                        {% if product.show %}
                            {{ include('/partials/_product.html.twig', {'product':product}) }}
                        {% endif %}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>



{% endblock %}