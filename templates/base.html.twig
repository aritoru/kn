<body>

<nav class="shadow-lg navbar sticky-top navbar-expand-lg navbar-dark bg-black header" style="background: black;">
    <a class="navbar-brand logo" href="{{ path('home') }}"> <img src="/img/fireheader.svg" width="40" height="40" alt="" class="d-inline-block align-middle">  KRPNFPN</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item">
                <a class="nav-link {% block band_block %} {% endblock %}" href="{{ path('band') }}">bio</a>
            </li>
            <!--
            <li class="nav-item">
                <a class="nav-link {% block announces_block %} {% endblock %}" href="{{ path('announce_list') }}">noticias</a>
            </li>
            -->
            <li class="nav-item">
                <a class="nav-link {% block contact_block %} {% endblock %}" href="{{ path('contact') }}">contacto</a>
            </li>
            <li class="nav-item">
                <a class="nav-link {% block media_block %} {% endblock %}" href="{{ path('media') }}">media</a>
            </li>
            <li class="nav-item">
                <a class="nav-link {% block store_block %} {% endblock %}" href="{{ path('store') }}">tienda</a>
            </li>
            <li class="nav-item">
                <a class="nav-link {% block tour_block %} {% endblock %}" href="{{ path('tour') }}">conciertos</a>
            </li>
        </ul>

        <ul class="navbar-nav flex-row mr-auto">
            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                <div class="dropdown show">
                    <a class="px-2 btn btn-primary btn-dark dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        {{ app.user.email }}
                    </a>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                        <a class="dropdown-item" href="{{ path('profile') }}">mi perfil</a>
                        <a class="dropdown-item" href="{{ path('profile_purchases') }}">mis pedidos</a>
                        <a class="dropdown-item" href="{{ path('download_disc') }}">descarga digital</a>
                        <a class="dropdown-item" href="{{ path('app_logout') }}">Desconectar</a>
                    </div>
                </div>

            {% else %}
                <li class="nav-item margin-right"><a class="px-2 btn btn-primary btn-dark" href="{{ path('app_login') }}">mi zona</a></li>
            {% endif %}

        </ul>

        <ul class="navbar-nav flex-row">
            <li class="nav-item"><a class="nav-link px-2" href="https://www.facebook.com/Krenfen"><i class="fab fa-facebook"></i></a></li>
            <li class="nav-item"><a class="nav-link px-2" href="https://www.instagram.com/krenfen/"><i class="fab fa-instagram"></i></a></li>
            <li class="nav-item"><a class="nav-link px-2" href="https://twitter.com/krenfen"><i class="fab fa-twitter"></i></a></li>
            <li class="nav-item"><a class="nav-link px-2" href="https://www.youtube.com/user/knkrenfen"><i class="fab fa-youtube"></i></a></li>
            <li class="nav-item"><a class="nav-link px-2" href="https://open.spotify.com/artist/3WvzynCwqWRSBzivc6gWG4"><i class="fab fa-spotify"></i></a></li>
            <li class="nav-item"><a class="nav-link px-2" href="https://itunes.apple.com/ca/artist/krenfen/920014104"><i class="fab fa-itunes"></i></a></li>
        </ul>
